  "dependencies": {
    "@sapphire/snowflake": "^3.5.5",
    "@sentry/node": "^9.2.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.17.3",
    "djs-command-helper": "^4.1.2",
    "mongodb": "^6.13.1",
    "mongoose": "^8.10.0",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "randomstring": "^1.3.0"
  },
  "devDependencies": {
    "@types/node-schedule": "^2.1.7",
    "@types/randomstring": "^1.3.0",
    "supportmail-types": "github:The-LukeZ/supportmail-types"
