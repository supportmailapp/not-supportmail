{
  "name": "not-supportmail",
  "private": true,
  "version": "2.0.0",
  "description": "SupportMail Management Bot",
  "main": "./dist/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpx tsc && pnpm sentry:sourcemaps",
    "dev": "pnpx tsc && node --env-file=.env.dev ./dist/index.js",
    "start": "pnpm build && node --env-file=.env.production ./dist/index.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org lukez-dev --project not-supportmail --release=2.0.0 ./dist && sentry-cli sourcemaps upload --org lukez-dev --project not-supportmail --release=2.0.0 ./dist"
  },
  "author": "supportmailapp",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@sapphire/snowflake": "^3.5.5",
    "@sentry/cli": "^2.58.2",
    "@sentry/node": "^9.47.1",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.0",
    "djs-command-helper": "^4.2.2",
    "humanize-duration": "^3.33.1",
    "ky": "^1.14.0",
    "mongoose": "^8.20.0",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "@types/humanize-duration": "^3.27.4",
    "@types/node-schedule": "^2.1.8"
  },
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  }
}
