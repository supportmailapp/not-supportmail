{
  "autoThreadedChannels": {
    "<ChannelId>": {
      "blacklist": ["u-UserIdToBlacklist", "r-RoleIdToBlacklist"], // Examples: User: "u-123" ; Role: "r-456"
      "whitelist": ["u-UserIdToBlacklist", "r-RoleIdToBlacklist"],
      "notes": "some notes for something"
    }
  },
  "autoPublishChannels": {
    "<ChannelId>": {
      "blacklist": ["u-UserIdToBlacklist", "r-RoleIdToBlacklist"],
      "whitelist": ["u-UserIdToBlacklist", "r-RoleIdToBlacklist"],
      "pings": ["u-UserIdToBlacklist", "r-RoleIdToBlacklist"],
      "notes": "some notes for something"
    }
  },
  "flagRemoval": {
    "allowedChannels": ["channel-id1", "channel-id2"],
    "allowedUsers": ["global-user-ids", "to-bypass-the-filter"]
  },
  "autoReactions": {
    "<ChannelId>": {
      "emojis": [
        // Note: The order in which the emojis are listed here the emojis are applied
        {
          "id": "emoji-id", // When unicode, leave blank ("")
          "name": "emoji-name", // When unicode emoji, paste it in here
          "animated": false // Only used for custom emojis
        },
        {
          "id": "123",
          "name": "custom-animated-emoji-example",
          "animated": true
        },
        {
          "id": "",
          "name": "ðŸ˜€",
          "animated": false
        }
      ],
      "blacklist": ["u-UserIdToBlacklist", "r-RoleIdToBlacklist"],
      "whitelist": ["u-UserIdToBlacklist", "r-RoleIdToBlacklist"]
    }
  },
  "joinRoles": {
    "<RoleId>": {
      "notes": "some notes for something",
      "bot": true, // If true, only bots can get this role
      "delay": 0 // Delay in seconds before the role is assigned
    }
  },
  "developers": [
    {
      "id": "developer-user-id-1",
      "gender": "m" // "m" for male, "f" for female, "d" for diverse
    },
    {
      "id": "developer-user-id-2",
      "gender": "f"
    }
    // Add more developer user IDs as needed | This is needed for the NO_PINGS_AUTOMOD_RULE feature (read readme)
  ]
}
